model Parking {
  id           String        @id @default(uuid())
  name         String
  address      String
  pricePerHour Float         @map("price_per_hour")
  capacity     Int
  parkingSlots ParkingSlot[]
  createdAt    DateTime      @default(now()) @map("created_at")
  updatedAt    DateTime      @updatedAt @map("updated_at")

  @@map("parkings")
}
